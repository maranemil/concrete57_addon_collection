<?xml version="1.0"?>
<schema version="0.3">
    <table name="btTumblrEntry">
        <field name="bID" type="I">
            <key/>
            <unsigned/>
        </field>
        <field name="preferedImageWidth" type="I2">
            <unsigned/>
            <notnull/>
            <default value="500"/>
        </field>
        <field name="dateFormat" type="C" size="255">
            <descr></descr>
            <default value="jS F Y \a\t H:i"/>
        </field>
    </table>
</schema>